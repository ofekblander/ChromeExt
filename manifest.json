{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "web_accessible_resources": [
      "sql.js",
      "data.sqlite"
    ],
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],
    "main": {
      "scripts": ["sql.js"]
    }
    
  }
  